#include <stdio.h>
#include <stdlib.h>

typedef struct retire_info{
  int months;
  double contribution;
  double rate_of_return;
} _retire_info;

void retirement(int startAge, double initial, retire_info working, retire_info retired){
  double balance = initial +  working.contribution *(1.0+ working.rate_of_return);
  
  
}
